.dialog
  %h1 Sign in
  = form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f|
    .field
      = f.label :email
      = f.email_field :email
    .field
      = f.label :password
      = f.password_field :password
    .field
      %label.checkLabel{for: "user_remember_me"}
        = f.check_box :remember_me
        Remember Me
    .actions= f.submit "Sign in"
    
  = render "devise/shared/links"
